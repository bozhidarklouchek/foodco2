    "instructions": [
        {
            "number": 1,
            "instruction": "Gather all ingredients."
        },
        {
            "number": 2,
            "instruction": "Dotdash Meredith Food Studios"
        },
        {
            "number": 3,
            "instruction": "Sprinkle the chicken breasts with 2 teaspoons salt."
        },
        {
            "number": 4,
            "instruction": "Heat oil in a large skillet over high heat; partially cook the chicken in the hot oil in batches until completely browned on all sides."
        },
        {
            "number": 5,
            "instruction": "Transfer browned chicken breasts to a plate and set aside."
        },
        {
            "number": 6,
            "instruction": "Dotdash Meredith Food Studios"
        },
        {
            "number": 7,
            "instruction": "Reduce the heat to medium and add onion, garlic, and ginger to the oil remaining in the skillet. Cook and stir until onion turns soft and translucent, 5 to 8 minutes. Stir curry powder, cumin, turmeric, coriander, cayenne, and 1 tablespoon of water into the onion mixture; allow to heat together for about 1 minute while stirring."
        },
        {
            "number": 8,
            "instruction": "Dotdash Meredith Food Studios"
        },
        {
            "number": 9,
            "instruction": "Dotdash Meredith Food Studios"
        },
        {
            "number": 10,
            "instruction": "Add tomatoes, yogurt, 1 tablespoon chopped cilantro, and 1 teaspoon salt to the mixture; stir to combine."
        },
        {
            "number": 11,
            "instruction": "Dotdash Meredith Food Studios"
        },
        {
            "number": 12,
            "instruction": "Return chicken breast to the skillet along with any juices on the plate."
        },
        {
            "number": 13,
            "instruction": "Pour in 1/2 cup water and bring to a boil, turning the chicken to coat with the sauce."
        },
        {
            "number": 14,
            "instruction": "Sprinkle garam masala and 1 tablespoon cilantro over the chicken."
        },
        {
            "number": 15,
            "instruction": "Dotdash Meredith Food Studios"
        },
        {
            "number": 16,
            "instruction": "Cover the skillet and simmer until chicken breasts are no longer pink in the center and the juices run clear, about 20 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C)."
        },
        {
            "number": 17,
            "instruction": "Drizzle with lemon juice to serve."
        },
        {
            "number": 18,
            "instruction": "DOTDASH MEREDITH FOOD STUDIOS"
        }
    ]
