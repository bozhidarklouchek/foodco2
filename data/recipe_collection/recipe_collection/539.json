    "instructions": [
        {
            "number": 1,
            "instruction": "Heat the oil in a frying pan over a medium-high heat."
        },
        {
            "number": 2,
            "instruction": "Add the onion and cook for 5 minutes, stirring often, until the onion softens."
        },
        {
            "number": 3,
            "instruction": "Add the garlic and cook for a further minute."
        },
        {
            "number": 4,
            "instruction": "Add the carrot and mushrooms and cook for a further 2-3 minutes."
        },
        {
            "number": 5,
            "instruction": "Add the minced beef and cook for 5-6 minutes, until browned. Stir often, breaking up any large chunks of meat with a spatula."
        },
        {
            "number": 6,
            "instruction": "Add the wine. Bring to the boil and let it bubble for 2-3 minutes until reduced by three-quarters."
        },
        {
            "number": 7,
            "instruction": "Add the tomato puree, oregano, thyme, Worcestershire sauce, beef stock cube, tinned tomatoes, brown sugar, salt, and pepper. Stir and bring to the boil. Turn down the heat and simmer for 15 -20 minutes until thickened."
        },
        {
            "number": 8,
            "instruction": "Meanwhile, boiled a large pan of salted water."
        },
        {
            "number": 9,
            "instruction": "Add the spaghetti and cook for 10-12 minutes until al dente."
        },
        {
            "number": 10,
            "instruction": "Drain the spaghetti and divide between 6 bowls."
        },
        {
            "number": 11,
            "instruction": "Spoon the bolognese on top of the spaghetti and sprinkle on the spring onions and parmesan before serving."
        }
