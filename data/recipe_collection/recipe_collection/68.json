    "instructions": [
        {
            "number": 1,
            "instruction": "For the rice"
        },
        {
            "number": 2,
            "instruction": "Wash the rice and soak in water for 20 minutes."
        },
        {
            "number": 3,
            "instruction": "Add ghee in a pan."
        },
        {
            "number": 4,
            "instruction": "Add cloves, black cardamom and black pepper and fry for a minute."
        },
        {
            "number": 5,
            "instruction": "Drain the rice and add it in the pan along with 4 cups of water, lemon juice and salt."
        },
        {
            "number": 6,
            "instruction": "Cover the pan and cook the rice on low heat until it is ¾ th done."
        },
        {
            "number": 7,
            "instruction": "Remove the pan from heat and keep aside."
        },
        {
            "number": 8,
            "instruction": "Remove the pan from heat and keep aside."
        },
        {
            "number": 9,
            "instruction": "For the mushroom masala"
        },
        {
            "number": 10,
            "instruction": "Heat oil and ghee in a heavy bottom pan."
        },
        {
            "number": 11,
            "instruction": "Add cloves, black pepper, black pepper and cinnamon stick and fry them for a few seconds."
        },
        {
            "number": 12,
            "instruction": "Add onions and green chilli and fry until onions are browned."
        },
        {
            "number": 13,
            "instruction": "Now add ginger garlic paste and fry for 2-3 minutes till the raw smell is gone."
        },
        {
            "number": 14,
            "instruction": "Add coriander powder, red chilli powder, salt and turmeric powder and fry for 1-2 minutes."
        },
        {
            "number": 15,
            "instruction": "Add a tablespoon of water if required."
        },
        {
            "number": 16,
            "instruction": "Now add mushrooms and cover the pan and cook till they are done."
        },
        {
            "number": 17,
            "instruction": "Remove the cover and fry the mushroom till all the water has reduced."
        },
        {
            "number": 18,
            "instruction": "Add fresh coriander and mix well"
        },
        {
            "number": 19,
            "instruction": "Assembly"
        },
        {
            "number": 20,
            "instruction": "Transfer the partially cooked rice over the mushroom and spread evenly."
        },
        {
            "number": 21,
            "instruction": "Sprinkle fried onions, cashew nuts, saffron milk and coriander on top."
        },
        {
            "number": 22,
            "instruction": "Cover the lid of the pan and cook the biryani on very low heat for 30-35 minutes."
        },
        {
            "number": 23,
            "instruction": "Fluff the biryani gently using a fork."
        },
        {
            "number": 24,
            "instruction": "Serve hot."
        }
    ]
