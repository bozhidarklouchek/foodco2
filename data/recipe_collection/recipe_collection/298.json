    "instructions": [
        {
            "number": 1,
            "instruction": "In a medium bowl, whisk together Dijon, green onions, olive oil, honey and vinegar; season with salt and pepper, to taste; set aside 1/3 of the mixture."
        },
        {
            "number": 2,
            "instruction": "Place remaining 2/3 of the mixture in the refrigerator until ready to serve."
        },
        {
            "number": 3,
            "instruction": "In a gallon size Ziploc bag or large bowl, combine chicken and reserved Dijon mixture; marinate for at least 2 hours to 6 hours, turning the bag occasionally."
        },
        {
            "number": 4,
            "instruction": "Drain the chicken from the marinade."
        },
        {
            "number": 5,
            "instruction": "Preheat grill to medium heat."
        },
        {
            "number": 6,
            "instruction": "Brush chicken with canola oil; season with salt and pepper, to taste."
        },
        {
            "number": 7,
            "instruction": "Add chicken to grill, and cook, turning occasionally, until chicken is completely cooked through, reaching an internal temperature of 165 degrees F, about 10 minutes."
        },
        {
            "number": 8,
            "instruction": "Heat a large skillet over medium high heat."
        },
        {
            "number": 9,
            "instruction": "Add bacon and cook until brown and crispy, about 6-8 minutes."
        },
        {
            "number": 10,
            "instruction": "Drain excess fat; transfer bacon to a paper towel-lined plate."
        },
        {
            "number": 11,
            "instruction": "To assemble the salad, place romaine lettuce in a large bowl; top with chicken, bacon, strawberries, avocado and corn."
        },
        {
            "number": 12,
            "instruction": "Pour the Dijon dressing on top of the salad and gently toss to combine."
        },
        {
            "number": 13,
            "instruction": "Serve immediately."
        }
    ]
