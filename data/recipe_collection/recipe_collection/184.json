    "instructions": [
        {
            "number": 1,
            "instruction": "Place the haddock in a large frying pan, skin-side up."
        },
        {
            "number": 2,
            "instruction": "Pour over 500ml/18fl oz water, add the bay leaves and bring the water to a gentle simmer. Cook the fish for 8–10 minutes until it is just done and flakes easily."
        },
        {
            "number": 3,
            "instruction": "Drain in a colander set over a bowl, reserving the cooking liquor, and discard the bay leaves."
        },
        {
            "number": 4,
            "instruction": "Pour the cooking liquor into a saucepan and stir in the rice. Cover with a lid and bring to the boil. Reduce the heat and simmer the rice very gently for 10 minutes. Turn off the heat and leave the rice covered for 3–5 minutes more. By this time it should have absorbed all the fish liquor."
        },
        {
            "number": 5,
            "instruction": "While the rice is cooking, bring some water to the boil in a saucepan."
        },
        {
            "number": 6,
            "instruction": "Add the eggs and cook for 8 minutes."
        },
        {
            "number": 7,
            "instruction": "Drain them in a sieve under cold running water and when cool enough to handle, peel them carefully and set aside. Cook the peas, if using, in a small saucepan of boiling water and drain."
        },
        {
            "number": 8,
            "instruction": "Melt the butter with the oil in a large frying pan and cook the onion over a low heat for 5 minutes until well softened, stirring occasionally."
        },
        {
            "number": 9,
            "instruction": "Add the curry powder and cook for another 3 minutes, stirring constantly."
        },
        {
            "number": 10,
            "instruction": "Place the cooked rice into the pan and stir into the onions."
        },
        {
            "number": 11,
            "instruction": "Add the peas, cream, parsley and a few twists of ground black pepper."
        },
        {
            "number": 12,
            "instruction": "Flake the fish into chunky pieces and add these to the pan. Gently stir in the lemon juice and cook for 1–2 minutes."
        },
        {
            "number": 13,
            "instruction": "Cut the eggs into quarters and place them on top of the rice. Cover the pan with a lid and heat through for 2–3 minutes or until the eggs are warm, then serve."
        }
    ]
