    "instructions": [
        {
            "number": 1,
            "instruction": "To make the pastry, sift the flour together with a pinch of salt in a large bowl. Rub in the butter until you have a soft breadcrumb texture."
        },
        {
            "number": 2,
            "instruction": "Add enough cold water to make the crumb mixture come together to form a firm dough, and then rest it in the fridge for 30 minutes."
        },
        {
            "number": 3,
            "instruction": "Roll out the pastry on a light floured surface and line a 22cm/8½in well-buttered flan dish. Don't cut off the edges of the pastry yet. Chill again."
        },
        {
            "number": 4,
            "instruction": "Preheat the oven to 190C/170C Fan/Gas"
        },
        {
            "number": 5,
            "instruction": "Remove the pastry case from the fridge and line the base of the pastry with baking parchment and then fill it with baking beans."
        },
        {
            "number": 6,
            "instruction": "Place on a baking tray and bake blind for 20 minutes."
        },
        {
            "number": 7,
            "instruction": "Remove the beans and parchment and return to the oven for another five minutes to cook the base"
        },
        {
            "number": 8,
            "instruction": "Reduce the temperature of the oven to 160C/140C Fan/Gas"
        },
        {
            "number": 9,
            "instruction": "Sprinkle the cheese into the pastry base and add the sliced tomatoes if you are using them. Fry the bacon pieces until crisp and sprinkle over them over the top."
        },
        {
            "number": 10,
            "instruction": "Combine the eggs with the milk and cream in a bowl and season well."
        },
        {
            "number": 11,
            "instruction": "Pour over the bacon and cheese."
        },
        {
            "number": 12,
            "instruction": "Sprinkle the thyme over the top and trim the edges of the pastry."
        },
        {
            "number": 13,
            "instruction": "Bake for 30–40 minutes, or until set. Allow to cool and set further."
        },
        {
            "number": 14,
            "instruction": "Trim the pastry edges to get a perfect edge and then serve in wedges."
        }
    ]
}