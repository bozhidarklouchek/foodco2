    "instructions": [
        {
            "number": 1,
            "instruction": "Heat coconut oil in a large stockpot or Dutch oven over medium heat."
        },
        {
            "number": 2,
            "instruction": "Add bell peppers and onion, and cook, stirring occasionally, until just tender, about 3-4 minutes. Stir in red curry paste, ginger and garlic until fragrant, about 1 minute."
        },
        {
            "number": 3,
            "instruction": "Gradually whisk in chicken stock, tomato sauce, fish sauce and brown sugar. Bring to a boil; reduce heat and simmer until flavors have blended, about 10-15 minutes."
        },
        {
            "number": 4,
            "instruction": "Stir in coconut milk and simmer until reduced and slightly thickened, about 10 minutes."
        },
        {
            "number": 5,
            "instruction": "Stir in shrimp, and cook, stirring occasionally, until pink and cooked through, about 3-4 minutes."
        },
        {
            "number": 6,
            "instruction": "Remove from heat; stir in basil, cilantro and lime juice; season with salt and pepper, to taste."
        },
        {
            "number": 7,
            "instruction": "Serve immediately."
