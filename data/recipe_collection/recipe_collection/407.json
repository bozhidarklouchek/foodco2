    "instructions": [
        {
            "number": 1,
            "instruction": "Salt the pheasant legs and let sit at room temperature for 30 minutes."
        },
        {
            "number": 2,
            "instruction": "Heat 2 tablespoons of the butter in a Dutch oven or another large, oven-proof pan over medium heat."
        },
        {
            "number": 3,
            "instruction": "Add the apple slices and sauté until they turn a little brown around the edges, turning occasionally."
        },
        {
            "number": 4,
            "instruction": "Sprinkle the apple slices with a little salt. Set aside on paper towels."
        },
        {
            "number": 5,
            "instruction": "Dust the pheasant in flour and add the remaining 2 tablespoons of butter to the pan. Brown the pheasant for 3 to 5 minutes per side."
        },
        {
            "number": 6,
            "instruction": "Remove from pan and set aside."
        },
        {
            "number": 7,
            "instruction": "Add the onion and increase the heat to medium-high. Sauté, stirring occasionally, until they just begin to brown, about 5 to 8 minutes."
        },
        {
            "number": 8,
            "instruction": "Take the pan off the heat and pour in the brandy."
        },
        {
            "number": 9,
            "instruction": "Put the pan back on the heat and, using a wooden spoon, scrape any browned bits off the bottom of the pan."
        },
        {
            "number": 10,
            "instruction": "Let the brandy boil until it has reduced by about half."
        },
        {
            "number": 11,
            "instruction": "Add the cider and bring it to a boil."
        },
        {
            "number": 12,
            "instruction": "Add the thyme."
        },
        {
            "number": 13,
            "instruction": "Lay the pheasant legs in the pan, cover and simmer gently until tender, anywhere from 90 minutes to 2 1/2 hours."
        },
        {
            "number": 14,
            "instruction": "Fish out the pheasant legs and strip the meat off the bones if you want to. Turn the heat to high, add the apples and boil down the sauce by half. When the sauce gets a little syrupy, turn off the heat and add the cream."
        },
        {
            "number": 15,
            "instruction": "Add salt to taste."
        },
        {
            "number": 16,
            "instruction": "Serve by spooning some apples and onions on everyone's plate and topping it with the pheasant."
        }
    ]
