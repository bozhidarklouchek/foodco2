    "instructions": [
        {
            "number": 1,
            "instruction": "Bring 8 cups of water to boil, add the chicken along with the bay leaves, salt and onion. Cook for 20 minutes until chicken is tender and no longer pink."
        },
        {
            "number": 2,
            "instruction": "Remove chicken and set aside. Sift the chicken broth and set aside."
        },
        {
            "number": 3,
            "instruction": "Preheat the oven to 400Â°F, place the onions on a baking sheet and drizzle 2 tablespoons of the olive oil on top of the onions. Roast in the oven until browned, about 30 minutes."
        },
        {
            "number": 4,
            "instruction": "Pick through molokhia to remove any stems or debris. Soak for 15 minutes and rinse a few times. Squeeze out any liquid and set aside."
        },
        {
            "number": 5,
            "instruction": "Heat the remaining 2 tablespoons olive oil in the pot used to make the chicken broth."
        },
        {
            "number": 6,
            "instruction": "Add the cilantro and garlic and cook until fragrant, about one minute."
        },
        {
            "number": 7,
            "instruction": "Add the molokhia to the mixture along with the 7 Spice, coriander, salt and pepper, and saute until the molokhia softens and becomes fragrant, about 5 minutes."
        },
        {
            "number": 8,
            "instruction": "Pour the chicken broth on top of the molokhia and cook on medium heat covered for 30 minutes."
        },
        {
            "number": 9,
            "instruction": "Lower the heat to simmer, add the roasted onions, shredded chicken, lemon juice and lemon slices, and simmer for 20 minutes."
        },
        {
            "number": 10,
            "instruction": "Serve warm over Lebanese rice, if desired."
        }
