    "instructions": [
        {
            "number": 1,
            "instruction": "Heat 2 tbsp olive or rapeseed oil in a large heavy-based pan."
        },
        {
            "number": 2,
            "instruction": "Add 1 finely chopped onion and cook gently for 5 minutes."
        },
        {
            "number": 3,
            "instruction": "Add 2 finely chopped medium celery sticks, 1 chopped yellow pepper and 1 chopped red pepper and cook for a further 5 mins."
        },
        {
            "number": 4,
            "instruction": "Add 6 chorizo sausages and 6 pork sausages and fry for 5 minutes."
        },
        {
            "number": 5,
            "instruction": "Stir in 3 chopped garlic cloves, 1 ½ tsp sweet smoked paprika, ½ tsp ground cumin and 1 tbsp dried thyme and continue cooking for 1 – 2 mins or until the aromas are released."
        },
        {
            "number": 6,
            "instruction": "Pour in 125ml white wine and use a wooden spoon to remove any residue stuck to the pan."
        },
        {
            "number": 7,
            "instruction": "Add two 400g cans of tomatoes, and 2 sprigs of fresh thyme and bring to a simmer. Crumble in the chicken stock cube and stir."
        },
        {
            "number": 8,
            "instruction": "Cook for 40 minutes. Stir in a 400g drained and rinsed can of aduki beans and cook for a further five minutes."
        },
        {
            "number": 9,
            "instruction": "Remove the thyme sprigs, season with black pepper and stir through some snipped chives, if using."
        },
        {
            "number": 10,
            "instruction": "Serve."
        }
    ]
