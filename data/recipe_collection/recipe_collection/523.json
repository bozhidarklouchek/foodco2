    "instructions": [
        {
            "number": 1,
            "instruction": "Cut beef into thin strips. Marinate the beef strips with the all ingredients in the marinate list."
        },
        {
            "number": 2,
            "instruction": "Mix well and marinate for 15 mins."
        },
        {
            "number": 3,
            "instruction": "In a small bowl mix all the ingredients from the stir fry sauce list and set aside."
        },
        {
            "number": 4,
            "instruction": "Break one egg into the the marinated beef strips."
        },
        {
            "number": 5,
            "instruction": "Mix well."
        },
        {
            "number": 6,
            "instruction": "Coat the beef strips with corn/potato flour. Make sure all the beef strips are seperated. Toss the excess flour out before frying."
        },
        {
            "number": 7,
            "instruction": "Heat the oil into hight heat about 350 F."
        },
        {
            "number": 8,
            "instruction": "Add the coated beef and deep fry for 1-2 mins till crispy."
        },
        {
            "number": 9,
            "instruction": "Remove from oil and transfer to paper towel or cooling rack."
        },
        {
            "number": 10,
            "instruction": "Heat the wok/pan to medium high heat."
        },
        {
            "number": 11,
            "instruction": "Drizzle a tablespoon of oil, add the cut onions and stir for few seconds. Then add the pepper slices, stir well."
        },
        {
            "number": 12,
            "instruction": "Add the fried beef strips."
        },
        {
            "number": 13,
            "instruction": "Pour the sauce mix, add the spring onions and fresh chillies. Stir well to combine all ingredients with high heat for 1- 2 mins."
        },
        {
            "number": 14,
            "instruction": "Transfer to serving plate."
        },
        {
            "number": 15,
            "instruction": "Serve immediately!"
        },
        {
            "number": 16,
            "instruction": "Match with hot steamed/cooked rice or stir fry noodle. Perfect as a savory snack too!"
        }
    ]
