    "instructions": [
        {
            "number": 1,
            "instruction": "Preheat the oven to 200C (180Â°C fan)."
        },
        {
            "number": 2,
            "instruction": "Make the spiced mince"
        },
        {
            "number": 3,
            "instruction": "Heat 2 tbsp of olive oil in a heavy-based saucepan."
        },
        {
            "number": 4,
            "instruction": "Add the onion, garlic and carrots."
        },
        {
            "number": 5,
            "instruction": "Fry for 5 minutes until soft and lightly coloured."
        },
        {
            "number": 6,
            "instruction": "Add the Quorn Meat Free Mince, tomatoes, oregano, cinnamon, mint and bay leaves."
        },
        {
            "number": 7,
            "instruction": "Season to taste. Bring to boil, reduce the heat, cover and simmer for 10 minutes, stirring occasionally."
        },
        {
            "number": 8,
            "instruction": "Remove the bay leaves and discard."
        },
        {
            "number": 9,
            "instruction": "Prepare the aubergine"
        },
        {
            "number": 10,
            "instruction": "Place the aubergine slices on absorbent kitchen paper to soak up excess moisture."
        },
        {
            "number": 11,
            "instruction": "Pop into a bowl and drizzle with the remaining oil on both sides. Set aside."
        },
        {
            "number": 12,
            "instruction": "Note: I cut my aubergine slices very thinly and find that, as per the original Quorn recipe, they do not need cooking ahead of being used in the moussaka. However, if you prefer thicker 1-1.5cm (half inch) slices, I recommend frying or gridding them in a hot pan for 30-60 seconds per side to soften."
        },
        {
            "number": 13,
            "instruction": "Make the white sauce"
        },
        {
            "number": 14,
            "instruction": "Pour the evaporated milk into a measuring jug and make-up to 1 litre with water."
        },
        {
            "number": 15,
            "instruction": "Put the cornflour in a small bowl, add 6 tbsp of the milk mixture and stir until lump-free. Set aside."
        },
        {
            "number": 16,
            "instruction": "Put the butter, remaining milk mixture and flour in a saucepan over a low heat."
        },
        {
            "number": 17,
            "instruction": "Whisk and slowly bring to the boil, stirring constantly with a whisk until all the lumps have gone."
        },
        {
            "number": 18,
            "instruction": "Reduce the heat and simmer for 3 minutes, then pour in the cornflour paste and continue to cook over a low heat until the sauce thickens."
        },
        {
            "number": 19,
            "instruction": "Remove from the heat and add the nutmeg."
        },
        {
            "number": 20,
            "instruction": "Season to taste then stir in the beaten egg."
        },
        {
            "number": 21,
            "instruction": "Assemble the vegetarian moussaka"
        },
        {
            "number": 22,
            "instruction": "Grease a large roasting tray. Arrange the mince and aubergine slices in alternating layers."
        },
        {
            "number": 23,
            "instruction": "Finish with a final layer of aubergine slices, then pour the white sauce evenly over the top."
        },
        {
            "number": 24,
            "instruction": "Sprinkle with the grated cheese."
        },
        {
            "number": 25,
            "instruction": "Bake, uncovered, in a hot oven for about 30 minutes until the top is a rich golden brown and the filling is piping hot."
        },
        {
            "number": 26,
            "instruction": "Leave to stand at room temperature for 15 minutes before serving. This allows everything to firm up."
        },
        {
            "number": 27,
            "instruction": "Serve warm."
        }
    ]
