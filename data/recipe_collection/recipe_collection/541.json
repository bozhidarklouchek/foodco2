    "instructions": [
        {
            "number": 1,
            "instruction": "Spray a deep frying pan over a medium-high heat with spray oil."
        },
        {
            "number": 2,
            "instruction": "Add the chicken, paprika, onion powder, garlic powder, salt and black pepper"
        },
        {
            "number": 3,
            "instruction": "Fry until lightly browned, remove and set aside."
        },
        {
            "number": 4,
            "instruction": "Spray the frying pan with some more spray oil"
        },
        {
            "number": 5,
            "instruction": "Add the onion and a little bit of the stock to deglaze the pan."
        },
        {
            "number": 6,
            "instruction": "Fry the onion until translucent and softened."
        },
        {
            "number": 7,
            "instruction": "Add the garlic and continue to fry for a further 1 min."
        },
        {
            "number": 8,
            "instruction": "Add the zucchini and red pepper. Stir to combine"
        },
        {
            "number": 9,
            "instruction": "Add back in the chicken and stock and simmer to reduce down a little."
        },
        {
            "number": 10,
            "instruction": "In the meantime cook the pasta according to package instructions until al dente and drain."
        },
        {
            "number": 11,
            "instruction": "Add the sriracha sauce to the chicken and vegetables, stir to combine."
        },
        {
            "number": 12,
            "instruction": "Add the pasta, cream cheese and sweet chilli sauce."
        },
        {
            "number": 13,
            "instruction": "Toss till combined and the sauce is lovely and creamy and velvety (couple of minutes)."
        },
        {
            "number": 14,
            "instruction": "Season with salt and black pepper to taste if needed, stir through some chopped parsley or cilantro (optional)."
        },
        {
            "number": 15,
            "instruction": "Serve and enjoy!!"
        }
    ]
