    "instructions": [
        {
            "number": 1,
            "instruction": "Peel and roughly chop the onions and carrots."
        },
        {
            "number": 2,
            "instruction": "Trim and roughly chop the celery."
        },
        {
            "number": 3,
            "instruction": "Peel and finely chop the garlic cloves."
        },
        {
            "number": 4,
            "instruction": "Pick the rosemary leaves, discard the stalks. Finely chop half of leaves, put the rest to one side."
        },
        {
            "number": 5,
            "instruction": "Drain the lentils."
        },
        {
            "number": 6,
            "instruction": "Heat a large pan on a medium heat."
        },
        {
            "number": 7,
            "instruction": "Add a good lug of olive oil and the onions, carrots, celery and garlic. Cook gently with the lid on for 8 to 10 minutes, stirring occasionally, or until softened."
        },
        {
            "number": 8,
            "instruction": "Add the chopped rosemary leaves, tinned tomatoes and stock and stir well. Bring to the boil, then add the drained lentils, reduce to a low heat and simmer gently for 30 minutes, or until thick and delicious."
        },
        {
            "number": 9,
            "instruction": "Peel the potatoes, cut them into halves and quarters depending on their size, and put them into a pan of salted, boiling water. Boil for about 10 minutes until tender. Stick a knife into them to check they’re soft all the way through."
        },
        {
            "number": 10,
            "instruction": "Drain in a colander and return them to the pan."
        },
        {
            "number": 11,
            "instruction": "Grate the cheese."
        },
        {
            "number": 12,
            "instruction": "Add the milk, butter, most of the cheese and a pinch of salt and pepper to the pan. Finely grate in a few scrapings of nutmeg. Mash until smooth and creamy."
        },
        {
            "number": 13,
            "instruction": "Preheat the oven to 190ºC/375ºF/gas mark"
        },
        {
            "number": 14,
            "instruction": "Transfer the lentil mixture to a large ovenproof baking dish. Spoon the mash evenly over the top, poke the remaining rosemary leaves into it to decorate and drizzle with olive oil."
        },
        {
            "number": 15,
            "instruction": "Sprinkle with the remaining grated cheese, then cook in the hot oven for 30 minutes, or until golden and bubbling."
        },
        {
            "number": 16,
            "instruction": "Serve with broccoli or some lovely peas."
        }
    ]
